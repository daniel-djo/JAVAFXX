<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.game.controllers.GameFightController">
    <center>
        <StackPane id="gamefightpicture">
            <HBox alignment="CENTER" spacing="100">
                <Label fx:id="attackerpicture" id="attackerpicture" />
                <Label fx:id="defenderpicture" id="defenderpicture" />
            </HBox>
        </StackPane>
    </center>
</BorderPane>
